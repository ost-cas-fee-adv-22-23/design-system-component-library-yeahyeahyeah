{
  "name": "@smartive-education/design-system-component-library-yeahyeahyeah",
  "version": "1.1.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com",
    "tag": "latest"
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "tailwind.config.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/cjs/index.js",
      "require": {
        "default": "./dist/cjs/index.js"
      }
    },
    "./tailwind.config.js": "./tailwind.config.js"
  },
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "del ./dist && del ./index.ts && ctix create --overwrite --noBackup --project ./tsconfig.dist.json && tsc -p tsconfig.dist.json && yarn build:cjs && yarn copy-files",
    "build:local": "del ./dist && del ./index.ts && ctix create --overwrite --noBackup --project ./tsconfig.dist.json && tsc -p tsconfig.dist.json && yarn build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "watch": "del ./dist && del ./index.ts && ctix create --overwrite --noBackup --project ./tsconfig.dist.json && tsc --watch -p tsconfig.dist.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "copy-files": "del ./tailwind.config.js && cp ../../tailwind.config.js ./",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-typescript": "^7.18.6",
    "@smartive/eslint-config": "^3.1.1",
    "@smartive/prettier-config": "^3.0.0",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/jest": "^0.0.10",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.5.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/storybook-readme": "^5.0.6",
    "@types/styled-components": "^5.1.26",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-styled-components": "^2.0.7",
    "ctix": "^1.8.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.29.0",
    "lodash.debounce": "^4.0.8",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "storybook-readme": "^5.0.9",
    "styled-components": "^5.3.6",
    "tailwindcss": "~3.2.7",
    "twin.macro": "^3.1.0",
    "types": "^0.1.1",
    "typescript": "^4.9.3",
    "web-vitals": "^2.1.4",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3"
  },
  "overrides": {
    "enhanced-resolve": "5.10.0"
  },
  "license": "MIT"
}
